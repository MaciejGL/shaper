{
  "name": "hypertro",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "PORT=${PORT:=4000} concurrently \"next dev --turbopack\" \"npm run codegen\"",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' prisma generate && next build",
    "start": "next start",
    "lint": "concurrently \"next lint\" \"tsc --noEmit\" --kill-others-on-fail --success first",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --reporter=verbose",
    "mobile:dev": "cd expo-app && npx expo start",
    "mobile:web": "cd expo-app && npx expo start --web",
    "mobile:android": "cd expo-app && npx expo run:android",
    "mobile:ios": "cd expo-app && npx expo run:ios",
    "mobile:clear": "cd expo-app && npx expo start --clear",
    "expo:build": "cd expo-app && eas build --platform all",
    "expo:submit": "cd expo-app && eas submit --platform all",
    "expo:update": "cd expo-app && eas update --branch production",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev --create-only",
    "codegen": "graphql-codegen --config codegen.ts --watch",
    "seed": "npx prisma db seed",
    "stripe:listen": "stripe listen --forward-to localhost:4000/api/stripe/webhooks",
    "analyze": "ANALYZE=true npm run build",
    "analyze:dev": "ANALYZE=true npm run dev",
    "hooks:install": "bash scripts/install-git-hooks.sh",
    "prepare": "bash scripts/install-git-hooks.sh"
  },
